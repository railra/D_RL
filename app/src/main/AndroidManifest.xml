            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.hl_3.MainActivity" />
        <activity android:name="com.example.hl_3.Register" />
    </application>

</manifest>